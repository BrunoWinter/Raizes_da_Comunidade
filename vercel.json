{
  "version": 2,
  "routes": [
    {
      "src": "/media/(.*)",
      "dest": "Raizes_da_Comunidade/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Raizes_da_Comunidade/setup/wsgi.py"
    }
  ],
  "frameworks": {
    "python": {
      "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
      "rootDirectory": "Raizes_da_Comunidade"
    }
  }
}
