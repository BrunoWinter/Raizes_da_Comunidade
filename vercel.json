{
  "version": 2,
  "builds": [
    {
      "src": "Raizes_da_Comunidade/setup/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/media/(.*)",
      "dest": "Raizes_da_Comunidade/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Raizes_da_Comunidade/setup/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "nome_do_seu_projeto.settings"
  },
  "functions": {
    "Raizes_da_Comunidade/setup/wsgi.py": {
      "runtime": "python3.12",
      "environment": {
        "DJANGO_SETTINGS_MODULE": "setup.settings"
      }
    }
  }
}
